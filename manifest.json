{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": ["storage", "declarativeNetRequest"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "options.html" 
    }
  }
  
